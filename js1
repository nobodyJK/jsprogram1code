          function cpxa(x,y,s,art,colors){
    for(var i = 0; i < art.length; i++) {
        for(var j = 0; j < art[i].length; j++) {
            var c = art[i][j];
            pla_gra.fillStyle = colors[c];
            pla_gra.fillRect(x + (j * s/art[i].length),y +( i * s/art[i].length), s/art[i].length+1, s/art[i].length+1);
    }
}}



