    function arc(x, y, w, h, s, t) {
        pla_gra.beginPath();
        pla_gra.ellipse(x, y, w / 2, h / 2, 0, s, t);
        pla_gra.stroke();
        pla_gra.closePath();
    };

    function ELLipse(x, y, w, h) {
        pla_gra.beginPath();
        pla_gra.ellipse(x, y, w / 2, h / 2, 0, 0, 2 * Math.PI);
        pla_gra.fill();
        pla_gra.stroke();
        pla_gra.closePath();
    };
    
