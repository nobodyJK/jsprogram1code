            //ellipse function draws an ellipse
            function ellipse (x, y, w, h) {
                pla_gra.beginPath();
                pla_gra.ellipse(x, y, w / 2, h / 2, 0, 0, 2 * Math.PI);
                pla_gra.fill();
                pla_gra.closePath();
            };



