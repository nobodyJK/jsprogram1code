            //}
            
            
            //[
            //function to get a random number
            function random (min, max) {
                return min + Math.random() * (max - min);
            }


            //function to make a blade af grass
            function blade (x, y) {
                this.g = random(-10, 10);

                pla_gra.beginPath();
                pla_gra.moveTo(x, y)
                pla_gra.quadraticCurveTo(x - this.g / 2, y - 10, x + this.g, y - 20 - random(0, 5));
                pla_gra.quadraticCurveTo(x + this.g / 2, y - 10, this.x + 5, y);
                pla_gra.fill()
                pla_gra.closePath();
            }

            
            pla_gra.save();


            pla_gra.translate(0, 500);

            
            //dirt{
            pla_gra.fillStyle = "rgb(77, 53, 8)"
            pla_gra.fillRect(0, 0, 100, 100)
            for (let i = 0; i < 100; i++) {

                pla_gra.fillStyle = "rgb(36, 15, 1)"
                ellipse(random(0, 100), random(0, 100), random(2, 10), random(2, 10));
                pla_gra.fillStyle = "rgb(51, 22, 3)"
                ellipse(random(0, 100), random(0, 100), random(2, 10), random(2, 10));
                pla_gra.fillStyle = "rgb(66, 28, 2)"
                ellipse(random(0, 100), random(0, 100), random(2, 10), random(2, 10));
                pla_gra.fillStyle = "rgb(77, 32, 2)"
                ellipse(random(0, 100), random(0, 100), random(2, 10), random(2, 10));
                pla_gra.fillStyle = "rgb(92, 40, 5)"
                ellipse(random(0, 100), random(0, 100), random(2, 10), random(2, 10));
            }
            //}
            
            
            //grass{

            let c = 0;
            for (let i = 0; i < 100; i++) {
                c = random(-30, 30)
                pla_gra.fillStyle = "rgb(" + (3 + c) + ", " + (99 + c) + ", " + (17 + c) + ")"
                blade(110 + random(0, 100), 30 + random(-4, 8));

            }


            //}


            //gold egg{
            pla_gra.save();
            pla_gra.fillStyle = "black"


            pla_gra.beginPath();
            pla_gra.moveTo(0, 400)
            pla_gra.quadraticCurveTo(-7, 254, 98, 237);
            pla_gra.quadraticCurveTo(206, 262, 200, 400);
            pla_gra.quadraticCurveTo(190, 497, 100, 500);
            pla_gra.quadraticCurveTo(10, 497, 0, 400);

            pla_gra.fill()
            pla_gra.closePath();

            pla_gra.clip();
            pla_gra.fillStyle = "rgb(245, 226, 101)"
            pla_gra.fillRect(0, 235, 200, 600);


            for (let i = 0; i < 100; i++) {
                pla_gra.fillStyle = "rgba(255, 255, 255, 2%)"
                ellipse(100, 312, i / 2, i * 1.5);
                pla_gra.fillStyle = "rgba(212, 175, 11, 2%)"
                ellipse(-100 + i, 400, i * 1, i * 2);
                ellipse(300 - i, 400, i * 1, i * 2);
                ellipse(100, 600 - i, i * 1.5, i * 1.5);

            }
            //}

            pla_gra.restore();



            //function for mist
            function mist (y) {
                let mist_gradient = pla_gra.createLinearGradient(300, y + 20, 300, y - 100);
                mist_gradient.addColorStop(1, 'rgb(171, 233, 255, 2%)');
                mist_gradient.addColorStop(0, 'rgb(9, 20, 171, 75%)');
                pla_gra.fillStyle = mist_gradient


                let up = random(0, 10);
                pla_gra.beginPath();
                pla_gra.moveTo(0, 600)
                pla_gra.lineTo(0, y - 50)

                for (let i = 1; i < 50; i++) {
                    let u = random(up - 10, up + 10)
                    pla_gra.lineTo(i * 12, y - 50 - u)
                    up = u
                }
                pla_gra.lineTo(600, y - 50)

                pla_gra.lineTo(600, 600)
                pla_gra.fill()
                pla_gra.closePath();
            }
            
            
            
            //function to make mountains idk why I named it hill XD
            function hill (y) {
                pla_gra.beginPath();

                pla_gra.moveTo(0, 600)
                pla_gra.lineTo(0, y - 20)


                for (let i = 1; i < 12; i++) {
                    pla_gra.lineTo(i * 50, y - random(-25, 100))

                }
                pla_gra.lineTo(600, y - 20)

                pla_gra.lineTo(600, 600)

                pla_gra.fill()
                pla_gra.closePath();

            }


            
            
            //mountains
            pla_gra.save();
            pla_gra.translate(0, 200);
            
            for (let i = 0; i < 5; i++) {
                let hill_gradient = pla_gra.createLinearGradient(300, 500 + i * 20 + 20, 300, 500 + i * 50 - 200);
                hill_gradient.addColorStop(1, "rgb(" + (69 + i * 20 + 20) + ", " + (45 + i * 18 + 20) + "," + (4 + i * 18 + 20) + ")");
                hill_gradient.addColorStop(0, "rgb(" + (69 + i * 20 - 40) + ", " + (45 + i * 18 - 40) + "," + (4 + i * 18 - 40) + ")");
                pla_gra.fillStyle = hill_gradient

                hill(500 + i * 20)

                if (i !== 4) {
                    mist(500 + i * 20);
                }
            }

            pla_gra.restore();
            pla_gra.restore();
          
          
