    function line(x1, y1, x2, y2) {
        pla_gra.beginPath();
        pla_gra.moveTo(x1, y1);
        pla_gra.lineTo(x2, y2);
        pla_gra.stroke();
        pla_gra.closePath();
    };
                
                
